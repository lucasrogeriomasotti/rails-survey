<p id="notice"><%= notice %></p>

<h1>Listing Question Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Rating scale</th>
      <th>Rating scale start</th>
      <th>Rating scale end</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @question_types.each do |question_type| %>
      <tr>
        <td><%= question_type.name %></td>
        <td><%= question_type.rating_scale %></td>
        <td><%= question_type.rating_scale_start %></td>
        <td><%= question_type.rating_scale_end %></td>
        <td><%= link_to 'Show', question_type %></td>
        <td><%= link_to 'Edit', edit_question_type_path(question_type) %></td>
        <td><%= link_to 'Destroy', question_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Question type', new_question_type_path %>
